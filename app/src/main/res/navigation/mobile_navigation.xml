            xmlns:tools="http://schemas.android.com/tools"
            android:id="@+id/mobile_navigation"
            app:startDestination="@+id/navigation_home">

    <fragment
            android:id="@+id/navigation_home"
            android:name="cs.project.ui.home.HomeFragment"
            android:label="@string/title_home"
            tools:layout="@layout/fragment_home"/>

    <fragment
            android:id="@+id/navigation_notes"
            android:name="cs.project.ui.note.NoteListFragment"
            android:label="@string/title_notes"
            tools:layout="@layout/fragment_note_list"/>

    <fragment
            android:id="@+id/navigation_task"
            android:name="cs.project.ui.task.TaskFragment"
            android:label="@string/title_task"
            tools:layout="@layout/fragment_task"/>
